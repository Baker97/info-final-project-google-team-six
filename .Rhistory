USRepsInZip <- function(zip.code){
resource <-"legislators/locate"
response <- GET(paste0(base.url,resource,"?zip=", zip.code))
body <- fromJSON(content(response, "text"))
new.frame <- data.frame(body)
reps <- select(new.frame, results.chamber, results.first_name, results.last_name, results.state, results.party, results.phone)
print("The representatives for this district are:")
state <- substr(reps$results.state, 1, 2)
party <- substr(reps$results.party, start = 1, stop = 1)
all.reps <- paste(reps$results.first_name," ", reps$results.last_name," (", state, "-", party, "). Phone: ", reps$results.phone, sep = "")
colnames(all.reps)[colnames(all.reps)=="1"]
View(all.reps)
list <- c(all.reps[1,], all.reps[2,], all.reps[3,])
print(list)
}
